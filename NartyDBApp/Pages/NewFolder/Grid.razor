@using Syncfusion.Blazor.Grids
@page "/Sprzet"

<SfGrid DataSource="@Items" TValue="Item">
    <GridEvents TValue="Item" on>

    </GridEvents>
</SfGrid>

@code {
    public List<DB.Models.Item> Items{ get; set; }

    protected override void OnInitialized(){
        Items = new();

        for (int i = 0; i < 10; i++)
        {
            DB.Models.Item item = new()
                {
                    Description = "aaaa",
                    ID = i,
                    Name = "witam"
                };
                Items.Add(item);
        }
    }
}
